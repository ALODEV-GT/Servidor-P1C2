switch ::= SWITCH PARENTESIS_A exp:h1 PARENTESIS_C LLAVE_A lista_casos:h2 LLAVE_C {: comList=new ArrayList(); comList.add(h1); comList.add(h2); RESULT = new NodoAST("SWITCH", comList, "sus_linea"); :}
;

lista_casos ::= lista_casos:h1 caso:h2  {: comList=new ArrayList<>(); comList.add(h2); RESULT = new NodoAST("LISTA_CASOS", unirArrayList(((NodoAST)h1).getHijos(), comList), "sus_linea"); :}
| caso:h1                               {: comList=new ArrayList<>(); comList.add(h1); RESULT = new NodoAST("LISTA_CASOS", comList, "sus_linea"); :}
| default:h1                            {: comList=new ArrayList<>(); comList.add(h1); RESULT = new NodoAST("LISTA_CASOS", comList, "sus_linea"); :}
| lista_casos:h1 default:h2             {: comList=new ArrayList<>(); comList.add(h2); RESULT = new NodoAST("LISTA_CASOS", unirArrayList(((NodoAST)h1).getHijos(), comList), "sus_linea"); :}
;

caso ::= CASO exp:h1 DOS_PUNTOS instrucciones:h2    {: comList=new ArrayList<>(); comList.add(h1); comList.add(h2); RESULT = new NodoAST("CASO", comList, "sus_linea"); :}
;

default ::= DEFAULT DOS_PUNTOS instrucciones:h1     {: comList=new ArrayList<>(); comList.add(h1); RESULT = new NodoAST("DEFAULT", comList, "sus_linea"); :}
;

instruccion_si ::= si:h1            {: comList=new ArrayList<>(); comList.add(h1); RESULT = new NodoAST("INSTRUCCION_SI", comList, "sus_linea"); :}
| si:h1 sino:h2                     {: comList=new ArrayList<>(); comList.add(h1); comList.add(h2); RESULT = new NodoAST("INSTRUCCION_SI", comList, "sus_linea"); :}
| si:h1 lista_sino_si:h2            {: comList=new ArrayList<>(); comList.add(h1); comList.add(h2); RESULT = new NodoAST("INSTRUCCION_SI", comList, "sus_linea"); :}
| si:h1 lista_sino_si:h2 sino:h3    {: comList=new ArrayList<>(); comList.add(h1); comList.add(h2); comList.add(h3); RESULT = new NodoAST("INSTRUCCION_SI", comList, "sus_linea"); :}
;

si ::= SI PARENTESIS_A exp:h1 PARENTESIS_C LLAVE_A instrucciones:h2 LLAVE_C {: comList=new ArrayList<>(); comList.add(h1); comList.add(h2); RESULT=new NodoAST("SI",comList,"sus_linea"); :}
;

sino ::= SINO LLAVE_A instrucciones:h1 LLAVE_C {: comList=new ArrayList<>(); comList.add(h1); RESULT=new NodoAST("SINO",comList,"sus_linea"); :}
;

sino_si ::= SINO SI PARENTESIS_A exp:h1 PARENTESIS_C  LLAVE_A instrucciones:h2 LLAVE_C {: comList=new ArrayList<>(); comList.add(h1); comList.add(h2); RESULT=new NodoAST("SINO_SI",comList,"sus_linea"); :}
;

lista_sino_si ::= lista_sino_si:h1 sino_si:h2 {: comList=new ArrayList<>(); comList.add(h2); RESULT = new NodoAST("LISTA_SINO_SI", unirArrayList(((NodoAST)h1).getHijos(), comList), "sus_linea"); :}
| sino_si:h1 {: comList=new ArrayList<>(); comList.add(h1); RESULT=new NodoAST("LISTA_SINO_SI",comList,"sus_linea"); :}
